    background: white;
    height: 80rpx;
    line-height: 80rpx;
  
}
.add_prize_image{
    margin-top: 15rpx;
}
.add_prize_image image{
    height:45rpx;
    width:45rpx;
   
}
.card_list{
    display: flex;
    flex-direction: row;
    background:white;
    height: 100rpx;
    line-height: 100rpx;
