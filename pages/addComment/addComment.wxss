}
.card_list_text{
    margin-left:30rpx;
}
.card_list_item{
    position:absolute;
    margin-top:-98rpx;
    margin-left:183rpx;
    color:#b4b8cc;

}
.add_comment_footer{
    display: flex;
    align-items: center;
    text-align: center;
    position: fixed;
    z-index: 999;
    height: 100rpx;
    line-height: 100rpx;
    background: #ff5289;
    bottom: 0rpx;
    left: 0rpx;
    right: 0rpx;    
    border-top: 1rpx solid #d9d9d9;
    border-bottom: 1rpx solid #d9d9d9;
   

}
.add_comment_footer_left{
   flex: 1;
   background: white;

}
.add_comment_footer_right{
    flex: 1;
